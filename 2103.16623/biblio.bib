@article{skokovChiralMagneticEffect2017,
  title = {Chiral {{Magnetic Effect Task Force Report}}},
  author = {Skokov, Vladimir and Sorensen, Paul and Koch, Volker and Schlichting, Soeren and Thomas, Jim and Voloshin, Sergei and Wang, Gang and Yee, Ho-Ung},
  year = {2017},
  month = jul,
  volume = {41},
  pages = {072001},
  issn = {1674-1137},
  doi = {10.1088/1674-1137/41/7/072001},
  abstract = {In this report, we briefly examine the current status of the study of the chiral magnetic effect including theory and experimental progress. We recommend future strategies for resolving uncertainties in interpretation including recommendations for theoretical work, recommendations for measurements based on data collected in the past five years, and recommendations for beam use in the coming years of RHIC. We have specifically investigated the case for colliding nuclear isobars (nuclei with the same mass but different charge) and find the case compelling. We recommend that a program of nuclear isobar collisions to isolate the chiral magnetic effect from background sources be placed as a high priority item in the strategy for completing the RHIC mission.},
  archiveprefix = {arXiv},
  eprint = {1608.00982},
  eprinttype = {arxiv},
  file = {/Users/jdb/Dropbox/bnl/docs/zotero/Chinese Physics C/2017/Skokov et al_Chinese Physics C_2017_Chiral Magnetic Effect Task Force Report.pdf;/Users/jdb/Zotero/storage/89B8ZKLI/1608.html},
  journal = {Chinese Phys. C},
  keywords = {High Energy Physics - Phenomenology,Nuclear Experiment,Nuclear Theory},
  number = {7}
}

