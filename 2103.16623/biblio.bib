@article{adlerRHICZeroDegree2001b,
  title = {The {{RHIC Zero Degree Calorimeter}}},
  author = {Adler, Clemens and Denisov, Alexei and Garcia, Edmundo and Murray, Michael and Strobele, Herbert and White, Sebastian},
  year = {2001},
  month = sep,
  volume = {A470},
  pages = {488--499},
  issn = {01689002},
  doi = {10.1016/S0168-9002(01)00627-1},
  abstract = {High Energy collisions of nuclei usually lead to the emission of evaporation neutrons from both ``beam'' and ``target'' nuclei. At the RHIC heavy ion collider with 100GeV/u beam energy, evaporation neutrons diverge by less than \$\textasciitilde 2\$ milliradians from the beam axis Neutral beam fragments can be detected downstream of RHIC ion collisions (and a large aperture Accelerator dipole magnet) if \$\textbackslash theta\textbackslash leq\$ 4 mr but charged fragments in the same angular range are usually too close to the beam trajectory. In this 'zero degree' region produced particles and other secondaries deposit negligible energy when compared with that of beam fragmentation neutrons. The purpose of the RHIC zero degree calorimeters (ZDC's) is to detect neutrons emitted within this cone along both beam directions and measure their total energy (from which we calculate multiplicity). The ZDC coincidence of the 2 beam directions is a minimal bias selection of heavy ion collisions. This makes it useful as an event trigger and a luminosity monitor\textbackslash cite\{baltz\} and for this reason we built identical detectors for all 4 RHIC experiments. The neutron multiplicity is also known to be correlated with event geometry \textbackslash cite\{appel\} and will be used to measure collision centrality in mutual beam int eractions.},
  archiveprefix = {arXiv},
  eprint = {nucl-ex/0008005},
  eprinttype = {arxiv},
  file = {/Users/jdb/Dropbox/bnl/docs/zotero/Nuclear Instruments and Methods in Physics Research Section A Accelerators, Spectrometers, Detectors and Associated Equipment/2001/Adler et al_Nuclear Instruments and Methods in Physics Research Section A Accelerators, Spectrometers, Detectors and Associated Equipment_2001_The RHIC Zero Degree Calorimeter.pdf;/Users/jdb/Zotero/storage/F5WW76WJ/0008005.html},
  journal = {Nucl. Instrum. Meth.},
  keywords = {Nuclear Experiment},
  number = {3}
}

