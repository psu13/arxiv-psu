@article{asakawaElectricChargeSeparation2010,
  title = {Electric {{Charge Separation}} in {{Strong Transient Magnetic Fields}}},
  author = {Asakawa, Masayuki and Majumder, Abhijit and M{\"u}ller, Berndt},
  year = {2010},
  month = jun,
  volume = {81},
  pages = {064912},
  issn = {0556-2813, 1089-490X},
  doi = {10.1103/PhysRevC.81.064912},
  abstract = {We discuss various mechanisms for the creation of an asymmetric charge fluctuation with respect to the reaction plane among hadrons emitted in relativistic heavy-ion collisions. We show that such mechanisms exist in both, the hadronic gas and the partonic phases of QCD. The mechanisms considered here all require the presence of a strong magnetic field (the ``chiral magnetic effect''), but they do not involve parity or charge-parity violations. We analyze how a transient local electric current fluctuation generated by the chiral magnetic effect can dynamically evolve into an asymmetric charge distribution among the final-state hadrons in momentum space. We estimate the magnitude of the event-by-event fluctuations of the final-state charge asymmetry due to the partonic and hadronic mechanisms.},
  archiveprefix = {arXiv},
  eprint = {1003.2436},
  eprinttype = {arxiv},
  file = {/Users/jdb/Dropbox/bnl/docs/zotero/Physical Review C/2010/Asakawa et al_Physical Review C_2010_Electric Charge Separation in Strong Transient Magnetic Fields.pdf;/Users/jdb/Zotero/storage/MT38UL8W/1003.html},
  journal = {Phys. Rev. C},
  keywords = {High Energy Physics - Phenomenology,Nuclear Theory},
  number = {6}
}

@article{atlascollaborationExclusiveDimuonProduction2020,
  title = {Exclusive Dimuon Production in Ultraperipheral {{Pb}}+{{Pb}} Collisions at \$\textbackslash sqrt\{s\_\{\textbackslash mathrm\{\vphantom{\}\}\}}{{NN}}\vphantom\{\}\vphantom\{\}\vphantom\{\} = 5.02\$ {{TeV}} with {{ATLAS}}},
  author = { {ATLAS Collaboration} },
  year = {2020},
  month = nov,
  abstract = {Exclusive dimuon production in ultraperipheral collisions (UPC), resulting from photon-photon interactions in the strong electromagnetic fields of colliding high-energy lead nuclei, \$\textbackslash mathrm\{PbPb\}(\textbackslash gamma\textbackslash gamma) \textbackslash rightarrow \textbackslash mu\^+\textbackslash mu\^- (\textbackslash mathrm\{Pb\}\^\{(\textbackslash star)\}\textbackslash mathrm\{Pb\}\^\{(\textbackslash star)\} )\$, is studied using \$\textbackslash mathcal\{L\}\_\{\textbackslash mathrm\{int\}\} = 0.48\$ nb\$\^\{-1\}\$ of \$\textbackslash sqrt\{s\_\textbackslash mathrm\{NN\}\}=5.02\$ TeV lead-lead collision data at the LHC with the ATLAS detector. Dimuon pairs are measured in the fiducial region \$p\_\{\textbackslash mathrm\{T\}\textbackslash mu\} {$>$} 4\$ GeV, \$|\textbackslash eta\_\{\textbackslash mu\}| {$<$} 2.4\$, invariant mass \$m\_\{\textbackslash mu\textbackslash mu\} {$>$} 10\$ GeV, and \$p\_\{\textbackslash mathrm\{T,\textbackslash mu\textbackslash mu\}\} {$<$} 2\$ GeV. The primary background from single-dissociative processes is extracted from the data using a template fitting technique. Differential cross sections are presented as a function of \$m\_\{\textbackslash mu\textbackslash mu\}\$, absolute pair rapidity (\$|y\_\{\textbackslash mu\textbackslash mu\}|\$), scattering angle in the dimuon rest frame (\$|\textbackslash cos \textbackslash vartheta\^\{\textbackslash star\}\_\{\textbackslash mu\textbackslash mu\}|\$) and the colliding photon energies. The total cross section of the UPC \$\textbackslash gamma \textbackslash gamma \textbackslash rightarrow \textbackslash mu\^\{+\}\textbackslash mu\^\{-\}\$ process in the fiducial volume is measured to be \$\textbackslash sigma\_\{\textbackslash mathrm\{fid\}\}\^\{\textbackslash mu\textbackslash mu\} = 34.1 \textbackslash! \textbackslash pm \textbackslash! 0.3 \textbackslash mathrm\{(stat.)\} \textbackslash! \textbackslash pm \textbackslash! 0.7 \textbackslash mathrm\{(syst.)\}\$ \$\textbackslash mu\textbackslash mathrm\{b\}\$. Generally good agreement is found with calculations from STARlight, which incorporate the leading-order Breit-Wheeler process with no final-state effects, albeit differences between the measurements and theoretical expectations are observed. In particular, the measured cross sections at larger \$|y\_\{\textbackslash mu\textbackslash mu\}|\$ are found to be about 10-20\% larger in data than in the calculations, suggesting the presence of larger fluxes of photons in the initial state. Modification of the dimuon cross sections in the presence of forward and/or backward neutron production is also studied and is found to be associated with a harder incoming photon spectrum, consistent with expectations.},
  archiveprefix = {arXiv},
  eprint = {2011.12211},
  eprinttype = {arxiv},
  file = {/Users/jdb/Dropbox/bnl/docs/zotero/arXiv2011.12211 [hep-ex, physicsnucl-ex]/2020/ATLAS Collaboration_arXiv2011.12211 [hep-ex, physicsnucl-ex]_2020_Exclusive dimuon production in ultraperipheral Pb+Pb collisions at.pdf;/Users/jdb/Zotero/storage/BSVXIVCD/2011.html},
  journal = {arXiv:2011.12211 [hep-ex, physics:nucl-ex]},
  keywords = {High Energy Physics - Experiment,Nuclear Experiment},
  primaryclass = {hep-ex, physics:nucl-ex}
}

@article{atlascollaborationObservationCentralitydependentAcoplanarity2018,
  title = {Observation of Centrality-Dependent Acoplanarity for Muon Pairs Produced via Two-Photon Scattering in {{Pb}}+{{Pb}} Collisions at \$\textbackslash sqrt\{s\_\{\textbackslash mathrm\{\vphantom{\}\}\}}{{NN}}\vphantom\{\}\vphantom\{\}\vphantom\{\}=5.02\$ {{TeV}} with the {{ATLAS}} Detector},
  author = { {ATLAS Collaboration} },
  year = {2018},
  month = nov,
  volume = {121},
  pages = {212301},
  issn = {0031-9007, 1079-7114},
  doi = {10.1103/PhysRevLett.121.212301},
  abstract = {This Letter presents a measurement of \$\textbackslash gamma \textbackslash gamma \textbackslash rightarrow \textbackslash mu\^\{+\} \textbackslash mu\^\{-\}\$ production in Pb+Pb collisions recorded by the ATLAS detector at the Large Hadron Collider at \$\textbackslash sqrt\{s\_\{\textbackslash mathrm\{NN\}\}\}=5.02\$ TeV with an integrated luminosity of 0.49 nb\$\^\{-1\}\$. The azimuthal angle and transverse momentum correlations between the muons are measured as a function of collision centrality. The muon pairs are produced from \$\textbackslash gamma \textbackslash gamma\$ through the interaction of the large electromagnetic fields of the nuclei. The contribution from background sources of muon pairs is removed using a template fit method. In peripheral collisions, the muons exhibit a strong back-to-back correlation consistent with previous measurements of muon pair production in ultra-peripheral collisions. The angular correlations are observed to broaden significantly in central collisions. The modifications are qualitatively consistent with rescattering of the muons while passing through the hot matter produced in the collision.},
  archiveprefix = {arXiv},
  eprint = {1806.08708},
  eprinttype = {arxiv},
  file = {/Users/jdb/Dropbox/bnl/docs/zotero/Physical Review Letters/2018/ATLAS Collaboration_Physical Review Letters_2018_Observation of centrality-dependent acoplanarity for muon pairs produced via.pdf;/Users/jdb/Zotero/storage/37MEQIGC/1806.html},
  journal = {Phys. Rev. Lett.},
  keywords = {High Energy Physics - Experiment,Nuclear Experiment},
  number = {21}
}

