@article{starcollaborationObservationExcessPsi2019,
  title = {Observation of Excess {{J}}/\$\textbackslash psi\$ Yield at Very Low Transverse Momenta in {{Au}}+{{Au}} Collisions at \$\textbackslash sqrt\{s\_\{\textbackslash rm\{\vphantom{\}\}\}}{{NN}}\vphantom\{\}\vphantom\{\}\vphantom\{\} =\$ 200 {{GeV}} and {{U}}+{{U}} Collisions at \$\textbackslash sqrt\{s\_\{\textbackslash rm\{\vphantom{\}\}\}}{{NN}}\vphantom\{\}\vphantom\{\}\vphantom\{\} =\$ 193 {{GeV}}},
  author = {STAR Collaboration},
  year = {2019},
  month = sep,
  volume = {123},
  pages = {132302},
  issn = {0031-9007, 1079-7114},
  doi = {10.1103/PhysRevLett.123.132302},
  abstract = {We report on the first measurements of J/\$\textbackslash psi\$ production at very low transverse momentum (\$p\_\{T\} {$<\$$} 0.2 GeV/c) in hadronic Au+Au collisions at \$\textbackslash sqrt\{s\_\{\textbackslash rm\{NN\}\}\} =\$ 200 GeV and U+U collisions at \$\textbackslash sqrt\{s\_\{\textbackslash rm\{NN\}\}\} =\$ 193 GeV. Remarkably, the inferred nuclear modification factor of J/\$\textbackslash psi\$ at mid-rapidity in Au+Au (U+U) collisions reaches about 24 (52) for \$p\_\{T\} {$<\$$} 0.05 GeV/c in the 60-80\$\textbackslash\%\$ collision centrality class. This noteworthy enhancement cannot be explained by hadronic production accompanied by cold and hot medium effects. In addition, the \$dN/dt\$ distribution of J/\$\textbackslash psi\$ for the very low \$p\_\{T\}\$ range is presented for the first time. The distribution is consistent with that expected from the Au nucleus and shows a hint of interference. Comparison of the measurements to theoretical calculations of coherent production shows that the excess yield can be described reasonably well and reveals a partial disruption of coherent production in semi-central collisions, perhaps due to the violent hadronic interactions. Incorporating theoretical calculations, the results strongly suggest that the dramatic enhancement of J/\$\textbackslash psi\$ yield observed at extremely low \$p\_\{T\}\$ originates from coherent photon-nucleus interactions. In particular, coherently produced J/\$\textbackslash psi\$'s in violent hadronic collisions may provide a novel probe of the quark-gluon-plasma.},
  archiveprefix = {arXiv},
  eprint = {1904.11658},
  eprinttype = {arxiv},
  file = {/Users/jdb/Dropbox/bnl/docs/zotero/Physical Review Letters/2019/STAR Collaboration_Physical Review Letters_2019_Observation of excess J-$-psi$ yield at very low transverse momenta in Au+Au.pdf;/Users/jdb/Zotero/storage/45SSUE97/1904.html},
  journal = {Phys. Rev. Lett.},
  keywords = {High Energy Physics - Experiment},
  number = {13}
}

@article{starcollaborationProbingExtremeElectromagnetic2019b,
  title = {Probing {{Extreme Electromagnetic Fields}} with the {{Breit}}-{{Wheeler Process}}},
  author = { {STAR Collaboration} and Adam, J. and others},
  year = {2019},
  month = oct,
  abstract = {The Relativistic Heavy Ion Collider (RHIC) accelerates nuclei to ultra-relativistic velocities, producing some of the strongest known magnetic fields in the Universe (\$\{10\^\{14\}-10\^\{15\}\}\$ Tesla). The highly Lorentz-contracted Coulomb fields of the nuclei generate a flux of linearly polarized quasi-real photons that can interact via the Breit-Wheeler process to produce electron-positron pairs (\$\{\textbackslash gamma\textbackslash gamma \textbackslash rightarrow e\^+e\^-\}\$). Experimental data presented in this article are consistent with Breit-Wheeler theory across all measured differentials. The detected pairs are produced predominantly at low transverse momentum (\$P\_\{\textbackslash perp\}\$) with a smooth invariant mass distribution, with the individual \$\{e\^\textbackslash pm\}\$ preferentially aligned along the beam direction, and with a 4th-order modulation in azimuth between the \$\{e\^+e\^-\}\$ pair and \$\{e\^\textbackslash pm\}\$ momenta. The \$P\_\{\textbackslash perp\}\$ spectrum broadens from large to small impact parameters. Our observation opens new opportunities to study Quantum Chromodynamics under extreme conditions and provides a new tool for interdisciplinary study of extreme electromagnetic fields.},
  archiveprefix = {arXiv},
  eprint = {1910.12400},
  eprinttype = {arxiv},
  file = {/Users/jdb/Dropbox/bnl/docs/zotero/arXiv1910.12400 [nucl-ex]/2019/STAR Collaboration et al_arXiv1910.12400 [nucl-ex]_2019_Probing Extreme Electromagnetic Fields with the Breit-Wheeler Process2.pdf;/Users/jdb/Zotero/storage/GXT8CTUF/1910.html},
  journal = {arXiv:1910.12400 [nucl-ex]},
  keywords = {Nuclear Experiment},
  primaryclass = {nucl-ex}
}

