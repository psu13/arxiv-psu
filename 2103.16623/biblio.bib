@book{theatlascollaborationMeasurementNonexclusiveDimuon2019,
  title = {Measurement of Non-Exclusive Dimuon Pairs Produced via \$\textbackslash gamma\textbackslash gamma\$ Scattering in {{Pb}}+{{Pb}} Collisions at \$\textbackslash sqrt\{s\_\{\textbackslash mathrm\{\vphantom{\}\}\}}{{NN}}\vphantom\{\}\vphantom\{\}\vphantom\{\} = 5.02\$ {{TeV}} with the {{ATLAS}} Detector},
  editor = {{The ATLAS collaboration}},
  year = {2019},
  abstract = {Results are presented of a measurement of dimuons produced via \$\textbackslash gamma\textbackslash gamma\$ scattering processes in inelastic, non-ultra-peripheral Pb+Pb collisions at \$\textbackslash sqrt\{s\_\{\textbackslash mathrm\{NN\}\}\} = 5.02\$ TeV. The measurement uses ATLAS data from the 2015 and 2018 Pb+Pb data-taking periods at the LHC with an integrated luminosity of \$1.9\textasciitilde\textbackslash mathrm\{nb\}\^\{-1\}\$. The \$\textbackslash gamma\textbackslash gamma \textbackslash rightarrow \textbackslash mu\^+\textbackslash mu\^-\$ pairs are identified via selections on pair momentum asymmetry and acoplanarity, and the contribution from the heavy flavor decay background is estimated using a template fit method. The pair yields are measured differentially as functions of the centrality, average transverse-momentum (\$\textbackslash bar\{p\}\_\{\textbackslash mathrm\{T\}\}\$) and rapidity of the pair. The acoplanarity distribution in a given centrality interval is observed to vary with \$\textbackslash bar\{p\}\_\{\textbackslash mathrm\{T\}\}\$. In contrast, distributions of the perpendicular transverse momentum, \$k\_\textbackslash perp \textbackslash equiv \textbackslash bar\{p\}\_\{\textbackslash mathrm\{T\}\} |\textbackslash pi-\textbackslash Delta \textbackslash phi|\$, are nearly independent of the \$\textbackslash bar\{p\}\_\{\textbackslash mathrm\{T\}\}\$. The prior observation of a collision centrality-dependent broadening of the acoplanarity distribution is confirmed. Furthermore, the improved statistics of the measurement reveal a depletion in the number of muon pairs near zero acoplanarity or \$k\_\{\textbackslash perp\}\$ in central events, resulting in the distributions peaking at non-zero values of acoplanarity or \$k\_\{\textbackslash perp\}\$. Fits to the \$k\_\textbackslash perp\$ distributions are used to estimate the centrality dependence of the peak position. The most probable \$k\_\textbackslash perp\$ is shown to increase from the most peripheral to the most central collisions, reaching a value of \$k\_\{\textbackslash perp\}=36\textbackslash pm1\$ MeV in the 0--5\% most-central collisions. The ability of these measurements to qualitatively differentiate between different physical origins of the observed centrality and \$\textbackslash bar\{p\}\_\{\textbackslash mathrm\{T\}\}\$ dependence are discussed},
  file = {/Users/jdb/Zotero/storage/6ZFLLJ2D/ATLAS-CONF-2019-051.pdf},
  keywords = {Conference talks on ATLAS,Particle Physics - Experiment},
  language = {eng}
}

