@article{baltzHeavyionPartialBeam1996,
  title = {Heavy-Ion Partial Beam Lifetimes Due to {{Coulomb}} Induced Processes},
  author = {Baltz, A. J. and {Rhoades-Brown}, M. J. and Weneser, J.},
  year = {1996},
  month = oct,
  volume = {54},
  pages = {4233--4239},
  publisher = {{American Physical Society}},
  doi = {10.1103/PhysRevE.54.4233},
  abstract = {The magnitude of heavy-ion beam loss at the relativistic heavy ion collider (RHIC) and the large hadron collider (LHC) due to Coulomb induced bound-electron\textendash positron production and to Coulomb dissociation of the nucleus is evaluated. With the nominal design parameters, the associated partial beam lifetime of RHIC (assuming four intersection regions) is calculated as 7.7 h and that of LHC (one intersection region) as 5.3 h. \textcopyright{} 1996 The American Physical Society.},
  file = {/Users/jdb/Dropbox/bnl/docs/zotero/Physical Review E/1996/Baltz et al_Physical Review E_1996_Heavy-ion partial beam lifetimes due to Coulomb induced processes.pdf;/Users/jdb/Zotero/storage/I94BLICF/PhysRevE.54.html},
  journal = {Phys. Rev. E},
  number = {4}
}

@article{baltzPhysicsUltraperipheralCollisions2008,
  title = {The {{Physics}} of {{Ultraperipheral Collisions}} at the {{LHC}}},
  author = {Baltz, A. J. and Baur, G. and {d'Enterria}, D. and Frankfurt, L. and Gelis, F. and Guzey, V. and Hencken, K. and Kharlov, Yu and Klasen, M. and Klein, S. R. and Nikulin, V. and Nystrand, J. and Pshenichnov, I. A. and Sadovsky, S. and Scapparone, E. and Seger, J. and Strikman, M. and Tverskoy, M. and Vogt, R. and White, S. N. and Wiedemann, U. A. and Yepes, P. and Zhalov, M.},
  year = {2008},
  month = mar,
  volume = {458},
  pages = {1--171},
  issn = {03701573},
  doi = {10.1016/j.physrep.2007.12.001},
  abstract = {We discuss the physics of large impact parameter interactions at the LHC: ultraperipheral collisions (UPCs). The dominant processes in UPCs are photon-nucleon (nucleus) interactions. The current LHC detector configurations can explore small \$x\$ hard phenomena with nuclei and nucleons at photon-nucleon center-of-mass energies above 1 TeV, extending the \$x\$ range of HERA by a factor of ten. In particular, it will be possible to probe diffractive and inclusive parton densities in nuclei using several processes. The interaction of small dipoles with protons and nuclei can be investigated in elastic and quasi-elastic \$J/\textbackslash psi\$ and \$\textbackslash Upsilon\$ production as well as in high \$t\$ \$\textbackslash rho\^0\$ production accompanied by a rapidity gap. Several of these phenomena provide clean signatures of the onset of the new high gluon density QCD regime. The LHC is in the kinematic range where nonlinear effects are several times larger than at HERA. Two-photon processes in UPCs are also studied. In addition, while UPCs play a role in limiting the maximum beam luminosity, they can also be used a luminosity monitor by measuring mutual electromagnetic dissociation of the beam nuclei. We also review similar studies at HERA and RHIC as well as describe the potential use of the LHC detectors for UPC measurements.},
  archiveprefix = {arXiv},
  eprint = {0706.3356},
  eprinttype = {arxiv},
  file = {/Users/jdb/Dropbox/bnl/docs/zotero/Physics Reports/2008/Baltz et al_Physics Reports_2008_The Physics of Ultraperipheral Collisions at the LHC.pdf;/Users/jdb/Dropbox/Documents/shard/zotero/baltzPhysicsUltraperipheralCollisions2008.md;/Users/jdb/Zotero/storage/LL3ZM6TT/0706.html},
  journal = {Physics Reports},
  keywords = {High Energy Physics - Experiment,High Energy Physics - Phenomenology,Nuclear Experiment,Nuclear Theory},
  number = {1-3}
}

