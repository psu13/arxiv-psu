  issn = {03702693},
  doi = {10.1016/j.physletb.2015.07.078},
  abstract = {The density distributions of large nuclei are typically modeled with a Woods-Saxon distribution characterized by a radius \$R\_\{0\}\$ and skin depth \$a\$. Deformation parameters \$\textbackslash beta\$ are then introduced to describe non-spherical nuclei using an expansion in spherical harmonics \$R\_\{0\}(1+\textbackslash beta\_2Y\^0\_2+\textbackslash beta\_4Y\^0\_4)\$. But when a nucleus is non-spherical, the \$R\_\{0\}\$ and \$a\$ inferred from electron scattering experiments that integrate over all nuclear orientations cannot be used directly as the parameters in the Woods-Saxon distribution. In addition, the \$\textbackslash beta\_2\$ values typically derived from the reduced electric quadrupole transition probability B(E2)\$\textbackslash uparrow\$ are not directly related to the \$\textbackslash beta\_2\$ values used in the spherical harmonic expansion. B(E2)\$\textbackslash uparrow\$ is more accurately related to the intrinsic quadrupole moment \$Q\_\{0\}\$ than to \$\textbackslash beta\_2\$. One can however calculate \$Q\_0\$ for a given \$\textbackslash beta\_2\$ and then derive B(E2)\$\textbackslash uparrow\$ from \$Q\_0\$. In this paper we calculate and tabulate the \$R\_0\$, \$a\$, and \$\textbackslash beta\_2\$ values that when used in a Woods-Saxon distribution, will give results consistent with electron scattering data. We then present calculations of the eccentricity \$\textbackslash varepsilon\_2\$ and \$\textbackslash varepsilon\_3\$ with the new and old parameters. We demonstrate that \$\textbackslash varepsilon\_3\$ is particularly sensitive to \$a\$ and argue that using the incorrect value of \$a\$ has important implications for the extraction of \$\textbackslash eta/s\$ from the QGP created in Heavy Ion collisions.},
  archiveprefix = {arXiv},
  eprint = {1409.8375},
  eprinttype = {arxiv},
  file = {/Users/jdb/Dropbox/bnl/docs/zotero/Physics Letters B/2015/Shou et al_Physics Letters B_2015_Parameterization of deformed nuclei for Glauber modeling in relativistic heavy.pdf;/Users/jdb/Dropbox/bnl/docs/zotero/Physics Letters B/2015/Shou et al_Physics Letters B_2015_Parameterization of Deformed Nuclei for Glauber Modeling in Relativistic Heavy2.pdf;/Users/jdb/Dropbox/Documents/shard/zotero/shouParameterizationDeformedNuclei2015.md;/Users/jdb/Zotero/storage/BFBAZ59Y/Shou et al. - 2015 - Parameterization of Deformed Nuclei for Glauber Mo.pdf;/Users/jdb/Zotero/storage/25A79UUV/1409.html;/Users/jdb/Zotero/storage/7U5XJEAE/S0370269315005985.html;/Users/jdb/Zotero/storage/DZDEGTQD/1409.html},
  journal = {Physics Letters B},
  keywords = {High Energy Physics - Phenomenology,Nuclear Theory}
}

