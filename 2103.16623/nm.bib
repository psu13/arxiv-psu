	abstract = {Light-by-light scattering (γγ γγ) is a quantum-mechanical process that is forbidden in the classical theory of electrodynamics. This reaction is accessible at the Large Hadron Collider thanks to the large electromagnetic field strengths generated by ultra-relativistic colliding lead ions. Using 480 μb−1 of lead–lead collision data recorded at a centre-of-mass energy per nucleon pair of 5.02 TeV by the ATLAS detector, here we report evidence for light-by-light scattering. A total of 13 candidate events were observed with an expected background of 2.6 ± 0.7 events. After background subtraction and analysis corrections, the fiducial cross-section of the process Pb + Pb (γγ) Pb(∗) + Pb(∗)γγ, for photon transverse energy ET {\textgreater} 3 GeV, photon absolute pseudorapidity {\textbar}η{\textbar} {\textless} 2.4, diphoton invariant mass greater than 6 GeV, diphoton transverse momentum lower than 2 GeV and diphoton acoplanarity below 0.01, is measured to be 70 ± 24 (stat.) ± 17 (syst.) nb, which is in agreement with the standard model predictions.},
	language = {en},
	number = {9},
	urldate = {2019-05-02},
	journal = {Nat. Phys},
	author = {{ATLAS Collaboration} and Aaboud, M. and others},
	month = sep,
	year = {2017},
	keywords = {Experimental nuclear physics, Experimental particle physics},
	pages = {852--858},
	file = {Atlas Collaboration_Nature Physics_2017_Evidence for light-by-light scattering in heavy-ion collisions with the ATLAS.pdf:/Users/jdb/Dropbox/bnl/docs/zotero/Nature Physics/2017/Atlas Collaboration_Nature Physics_2017_Evidence for light-by-light scattering in heavy-ion collisions with the ATLAS.pdf:application/pdf;Snapshot:/Users/jdb/Zotero/storage/3H79UL2M/nphys4208.html:text/html}
}

