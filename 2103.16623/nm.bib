	file = {APS Snapshot:/Users/jdb/Zotero/storage/N4B7HQ4S/PhysRevLett.79.html:text/html;Burke et al_Physical Review Letters_1997_Positron Production in Multiphoton Light-by-Light Scattering.pdf:/Users/jdb/Dropbox/bnl/docs/zotero/Physical Review Letters/1997/Burke et al_Physical Review Letters_1997_Positron Production in Multiphoton Light-by-Light Scattering2.pdf:application/pdf}
