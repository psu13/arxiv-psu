@article{burke_positron_1997,
	title = {Positron {Production} in {Multiphoton} {Light}-by-{Light} {Scattering}},
	volume = {79},
	url = {https://link.aps.org/doi/10.1103/PhysRevLett.79.1626},
	doi = {10.1103/PhysRevLett.79.1626},
	abstract = {A signal of 106±14 positrons above background has been observed in collisions of a low-emittance 46.6 GeV electron beam with terawatt pulses from a Nd:glass laser at 527 nm wavelength in an experiment at the Final Focus Test Beam at SLAC. The positrons are interpreted as arising from a two-step process in which laser photons are backscattered to GeV energies by the electron beam followed by a collision between the high-energy photon and several laser photons to produce an electron-positron pair. These results are the first laboratory evidence for inelastic light-by-light scattering involving only real photons.},
	number = {9},
	urldate = {2019-07-22},
	journal = {Phys. Rev. Lett.},
