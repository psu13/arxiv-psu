@article{klein_two-photon_2018,
	title = {Two-photon production of dilepton pairs in peripheral heavy ion collisions},
	volume = {97},
	url = {https://link.aps.org/doi/10.1103/PhysRevC.97.054903},
	doi = {10.1103/PhysRevC.97.054903},
	abstract = {The STAR collaboration has observed an excess production of e+e− pairs in relativistic heavy ion collisions, over the expectations from hadronic production models. The excess pairs have transverse momenta pT{\textless}150MeV/c and are most prominent in peripheral gold-gold and uranium-uranium collisions. The pairs exhibit a peak at the J/ψ mass, but include a wide continuum, with pair invariant masses from 400MeV/c2 up to 2.6GeV/c2. The ALICE Collaboration observes a similar excess in peripheral lead-lead collisions, but only at the J/ψ mass, without a corresponding continuum. This paper presents a calculation of the cross section and kinematic for two-photon production of e+e− pairs, and find general agreement with the STAR data. The calculation is based on the starlight simulation code, which is based on the Weizsäcker-Williams virtual photon approach. The STAR continuum observations are compatible with two-photon production of e+e− pairs. The ALICE analysis required individual muon pT be greater than 1 GeV/c; this eliminated almost all of the pairs from two-photon interactions, while leaving most of the J/ψ decays.},
	number = {5},
	urldate = {2019-04-15},
	journal = {Phys. Rev. C},
	author = {Klein, Spencer R.},
	month = may,
	year = {2018},
	pages = {054903},
	file = {APS Snapshot:/Users/jdb/Zotero/storage/EP9UQNEE/PhysRevC.97.html:text/html;Klein_Physical Review C_2018_Two-photon production of dilepton pairs in peripheral heavy ion collisions.pdf:/Users/jdb/Dropbox/bnl/docs/zotero/Physical Review C/2018/Klein_Physical Review C_2018_Two-photon production of dilepton pairs in peripheral heavy ion collisions2.pdf:application/pdf}
}



@article{baltz_physics_2008,
	title = {The {Physics} of {Ultraperipheral} {Collisions} at the {LHC}},
	volume = {458},
	issn = {03701573},
	url = {http://arxiv.org/abs/0706.3356},
	doi = {10.1016/j.physrep.2007.12.001},
	abstract = {We discuss the physics of large impact parameter interactions at the LHC: ultraperipheral collisions (UPCs). The dominant processes in UPCs are photon-nucleon (nucleus) interactions. The current LHC detector configurations can explore small \$x\$ hard phenomena with nuclei and nucleons at photon-nucleon center-of-mass energies above 1 TeV, extending the \$x\$ range of HERA by a factor of ten. In particular, it will be possible to probe diffractive and inclusive parton densities in nuclei using several processes. The interaction of small dipoles with protons and nuclei can be investigated in elastic and quasi-elastic \$J/{\textbackslash}psi\$ and \${\textbackslash}Upsilon\$ production as well as in high \$t\$ \${\textbackslash}rho{\textasciicircum}0\$ production accompanied by a rapidity gap. Several of these phenomena provide clean signatures of the onset of the new high gluon density QCD regime. The LHC is in the kinematic range where nonlinear effects are several times larger than at HERA. Two-photon processes in UPCs are also studied. In addition, while UPCs play a role in limiting the maximum beam luminosity, they can also be used a luminosity monitor by measuring mutual electromagnetic dissociation of the beam nuclei. We also review similar studies at HERA and RHIC as well as describe the potential use of the LHC detectors for UPC measurements.},
	number = {1-3},
	urldate = {2019-04-15},
	journal = {Phys. Rep.},
	author = {Baltz, A. J. and Baur, G. and d'Enterria, D. and Frankfurt, L. and Gelis, F. and Guzey, V. and Hencken, K. and Kharlov, Yu and Klasen, M. and Klein, S. R. and Nikulin, V. and Nystrand, J. and Pshenichnov, I. A. and Sadovsky, S. and Scapparone, E. and Seger, J. and Strikman, M. and Tverskoy, M. and Vogt, R. and White, S. N. and Wiedemann, U. A. and Yepes, P. and Zhalov, M.},
	month = mar,
	year = {2008},
	keywords = {High Energy Physics - Experiment, High Energy Physics - Phenomenology, Nuclear Experiment, Nuclear Theory},
	pages = {1--171},
	file = {arXiv.org Snapshot:/Users/jdb/Zotero/storage/LL3ZM6TT/0706.html:text/html;Baltz et al_Physics Reports_2008_The Physics of Ultraperipheral Collisions at the LHC.pdf:/Users/jdb/Dropbox/bnl/docs/zotero/Physics Reports/2008/Baltz et al_Physics Reports_2008_The Physics of Ultraperipheral Collisions at the LHC.pdf:application/pdf}
