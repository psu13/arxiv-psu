isbn = {0031-9007$\backslash$n1079-7114},
issn = {0031-9007},
journal = {Physical Review Letters},
month = {mar},
number = {11},
pages = {110502},
pmid = {21469850},
publisher = {American Physical Society},
title = {{Observation of Quantum Jumps in a Superconducting Artificial Atom}},
url = {https://link.aps.org/doi/10.1103/PhysRevLett.106.110502},
volume = {106},
year = {2011}
}
@article{Plenio1998,
abstract = {Dissipation, the irreversible loss of energy and coherence, from a microsystem, is the result of coupling to a much larger macrosystem (or reservoir) which is so large that one has no chance of keeping track of all of its degrees of freedom. The microsystem evolution is then described by tracing over the reservoir states, resulting in an irreversible decay as excitation leaks out of the initially excited microsystems into the outer reservoir environment. Earlier treatments of this dissipation described an ensemble of microsystems using density matrices, either in Schroedinger picture with Master equations, or in Heisenberg picture with Langevin equations. The development of experimental techniques to study single quantum systems (for example single trapped ions, or cavity radiation field modes) has stimulated the construction of theoretical methods to describe individual realizations conditioned on a particular observation record of the decay channel, in the environment. These methods, variously described as Quantum Jump, Monte Carlo Wavefunction and Quantum Trajectory methods are the subject of this review article. We discuss their derivation, apply them to a number of current problems in quantum optics and relate them to ensemble descriptions.},
archivePrefix = {arXiv},
arxivId = {quant-ph/9702007},
author = {Plenio, M. B. and Knight, P. L.},
doi = {10.1103/RevModPhys.70.101},
eprint = {9702007},
isbn = {0034-6861},
issn = {0034-6861},
journal = {Reviews of Modern Physics},
month = {jan},
number = {1},
pages = {101--144},
pmid = {23698330},
primaryClass = {quant-ph},
publisher = {American Physical Society},
title = {{The quantum-jump approach to dissipative dynamics in quantum optics}},
