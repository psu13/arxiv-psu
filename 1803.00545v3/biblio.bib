publisher = {American Physical Society},
title = {{Cavity Attenuators for Superconducting Qubits}},
url = {https://link.aps.org/doi/10.1103/PhysRevApplied.11.014031 http://arxiv.org/abs/1807.04849},
volume = {11},
year = {2019}
}
