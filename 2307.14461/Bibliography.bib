@article{Birkhoff1939,
title = {A {{Characterization}} of {{Boolean Algebras}}},
author = {Birkhoff, Garrett and Ward, Morgan},
date = {1939},
journaltitle = {Annals of Mathematics},
volume = {40},
pages = {609--610},
abstract = {When is a lattice a Boolean algebra - that is, isomorphic with a field of sets? The classical conditions are: (1) The distributive law holds, (2) every element has a complement. Now it is well known that (1) and (2) imply (3) No element has more than one complement. We are thus led to conjecture that (2) and (3) imply (1); in other words, that a necessary and sufficient condition that a lattice be a Boolean algebra is that each of its elements have a unique complement. The only published result bearing on this question is G. Bergmann's theorem that the distributive law holds if and only if relative complements are unique; that is, if and only if given a \textbackslash textless= x \textbackslash textless= b, there exists one and only one y with x \^ y = a, x V y = b. We prove here the truth of our conjecture for all complete atomistic lattices. These include lattices of finite length and of finite order. We do not know whether or not our conjecture is unrestrictedly true},
keywords = {Boolean Algebra,Lattice Theory,Universal Algebra},
langid = {english},
number = {3}
}

