@article{Street1972,
title = {The {{Formal Theory}} of {{Monads}}},
author = {Street, Ross},
date = {1972-07},
journaltitle = {Journal of Pure and Applied Algebra},
volume = {2},
pages = {149--168},
issn = {00224049},
abstract = {We give an explicit description of the free completion EM ( K ) of a 2-category K under the EilenbergMoore construction, and show that this has the same underlying category as the 2-category Mnd ( K ) of monads in K . We then demonstrate that much of the formal theory of monads can be deduced using only the universal property of this completion, provided that one is willing to work with EM ( K ) as the 2-category of monads rather than Mnd ( K ) . We also introduce the wreaths in K ; these are the objects of EM ( EM ( K )) , and are to be thought of as generalized distributive laws. We study these wreaths, and give examples to show how they arise in a variety of contexts.},
langid = {english},
number = {2}
}

@thesis{Stubbe2003,
title = {Categorical {{Structures Enriched}} in a {{Quantaloid}}: {{Categories}} and {{Semicategories}}},
author = {Stubbe, Isar},
date = {2003},
institution = {{Universit\'e Catholique de Louvain}},
url = {http://www-lmpa.univ-littoral.fr/~stubbe/PDF/PhDIntroduction.pdf},
abstract = {Applying (enriched) categorical structures we define the notion of ordered sheaf on a quantaloid \textbackslash mathcal\{Q\} , which we call ` \textbackslash mathcal\{Q\} -order'. This requires a theory of semicategories enriched in the quantaloid \textbackslash mathcal\{Q\} , that admit a suitable Cauchy completion. There is a quantaloid \textbackslash mathsf\{Idl\}(\textbackslash mathcal\{Q\}) of \textbackslash mathcal\{Q\} -orders and ideal relations, and a locally ordered category \textbackslash mathsf\{Ord\}(\textbackslash mathcal\{Q\}) of \textbackslash mathcal\{Q\} -orders and monotone maps; actually, \textbackslash mathsf\{Ord\}(\textbackslash mathcal\{Q\})=\textbackslash mathsf\{Map\}(\textbackslash mathsf\{Idl\}(\textbackslash mathcal\{Q\})) . In particular is \textbackslash mathsf\{Ord\}(\textbackslash Omega) , with {$\Omega$} a locale, the category of ordered objects in the topos of sheaves on {$\Omega$}. In general \textbackslash mathcal\{Q\} -orders can equivalently be described as Cauchy complete categories enriched in the split-idempotent completion of \textbackslash mathcal\{Q\} . Applied to a locale {$\Omega$} this generalizes and unifies previous treatments of (ordered) sheaves on {$\Omega$} in terms of {$\Omega$}-enriched structures.},
langid = {english},
type = {PhD Thesis}
}

