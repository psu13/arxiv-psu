issn = {1050-2947},
journal = {Physical Review A},
month = {jul},
number = {1},
pages = {013814},
publisher = {American Physical Society},
title = {{Josephson-junction-embedded transmission-line resonators: From Kerr medium to in-line transmon}},
url = {http://link.aps.org/doi/10.1103/PhysRevA.86.013814 https://link.aps.org/doi/10.1103/PhysRevA.86.013814},
volume = {86},
year = {2012}
}
