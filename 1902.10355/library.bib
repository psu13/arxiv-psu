author = {Rybarczyk, Th{\'{e}}o and Gerlich, Stefan and Peaudecerf, Bruno and Penasa, Mariane and Julsgaard, Brian and Moelmer, Klaus and Gleyzes, S{\'{e}}bastien and Brune, Michel and Raimond, Jean-Michel and Haroche, Serge and Dotsenko, Igor},
