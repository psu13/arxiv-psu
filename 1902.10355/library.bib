arxivId = {1101.4576},
author = {Lecocq, Florent and Pop, Ioan M and Peng, Zhihui and Matei, Iulian and Crozes, Thierry and Fournier, Thierry and Naud, C{\'{e}}cile and Guichard, Wiebke and Buisson, Olivier},
